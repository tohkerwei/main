<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="tableView" prefHeight="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <columns>
    <TableColumn fx:id="id" editable="false" maxWidth="30.0" minWidth="30.0" prefWidth="30.0" reorderable="false" resizable="false" text="#" />
    <TableColumn fx:id="exerciseDate" editable="false" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" reorderable="false" resizable="false" text="Date" />
    <TableColumn fx:id="exerciseName" editable="false" minWidth="100.0" prefWidth="200.0" reorderable="false" text="Exercise" />
    <TableColumn fx:id="exerciseReps" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" reorderable="false" resizable="false" text="Reps" />
    <TableColumn fx:id="exerciseWeight" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" reorderable="false" resizable="false" text="Weight" />
    <TableColumn fx:id="exerciseSets" editable="false" maxWidth="75.0" minWidth="75.0" prefWidth="75.0" reorderable="false" resizable="false" text="Sets" />
  </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</TableView>
